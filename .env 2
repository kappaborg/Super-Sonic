# Database Configuration
POSTGRES_URL="postgresql://your_username:your_password@localhost:5432/your_database"

# App Configuration
NEXT_PUBLIC_APP_URL="http://localhost:3000"

# Auth Configuration
JWT_SECRET="your-jwt-secret-key"

# Optional: Other configurations
POSTGRES_PRISMA_URL="${POSTGRES_URL}?pgbouncer=true&connect_timeout=15"
POSTGRES_URL_NON_POOLING="${POSTGRES_URL}?connect_timeout=15" 